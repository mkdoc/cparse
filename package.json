{
  "name": "mkparse",
  "version": "1.5.9",
  "description": "Parse source file comments",
  "main": "index.js",
  "author": "muji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkdoc/mkparse/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mkdoc/mkparse.git"
  },
  "keywords": [
    "comment",
    "parser",
    "stream",
    "tag",
    "annotation"
  ],
  "dependencies": {
    "stream-lines": "~1.1.7",
    "through3": "~1.1.6"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5"
  },
  "scripts": {
    "clean": "rm -rf coverage",
    "docs": "npm run api && npm run example",
    "example": "mdp --force -v -i doc/example.json -o EXAMPLE.md",
    "lang": "node doc/languages.js > lang/index.js",
    "api": "mdp --force -v -i doc/api.json -o API.md",
    "lint": "jshint . && jscs .",
    "test": "NODE_env=test mocha ${SPEC:-test/spec/**}",
    "cover": "NODE_ENV=test istanbul cover _mocha -- ${SPEC:-test/spec/**}",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "man": {
      "example": true
    }
  }
}
