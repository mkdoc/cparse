{
  "name": "cparse",
  "version": "1.3.1",
  "description": "Generic comments parser",
  "main": "index.js",
  "author": "muji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmpfs/cparse/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmpfs/cparse.git"
  },
  "keywords": [
    "comment",
    "parser",
    "stream",
    "tag",
    "annotation"
  ],
  "dependencies": {
    "stream-lines": "~1.1.7",
    "through3": "~1.1.5"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5"
  },
  "scripts": {
    "clean": "rm -rf coverage",
    "readme": "mdp --force -v",
    "lint": "jshint . && jscs .",
    "api": "mdapi index.js c.js ./lib/*.js --level=2 --title=API",
    "test": "NODE_env=test mocha ${SPEC:-test/spec}",
    "cover": "NODE_ENV=test istanbul cover _mocha -- ${SPEC:-test/spec}",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "mdp": {
    "title": "Comment Parser",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/tmpfs/cparse",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "usage.md"
        ]
      },
      {
        "title": "Input",
        "inc": "../../test/example.js",
        "type": "code",
        "language": "javascript"
      },
      {
        "title": "Output",
        "bin": "INDENT=2 node test/spec test/example.js --json",
        "type": "code",
        "language": "json"
      },
      {
        "bin": "mdapi index.js c.js lib/*.js --level=2 --title=API"
      },
      {
        "inc": [
          "developer.md"
        ]
      }
    ]
  }
}
